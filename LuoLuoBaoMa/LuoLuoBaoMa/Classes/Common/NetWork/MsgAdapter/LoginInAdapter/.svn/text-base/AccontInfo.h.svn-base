@property(nonatomic,strong) NSString *    _strOpenId;     //企业id
@property(nonatomic,strong) NSString *    _strCompany;    //公司logo
@property(nonatomic,strong) NSString *    _strCompanyName;//公司名称
@property(nonatomic,strong) NSString *    _strJobNum;     //工号

@property(nonatomic,strong) NSString *    _strCreateTime; // 创建时间
@property(nonatomic,strong) NSString *    _strPhone; // 手机号
@property(nonatomic,strong) NSString *    _strEmail;//邮箱
@property(nonatomic,strong) NSMutableArray  *    _arrvacationType;//请假事项
@property(nonatomic,strong) NSMutableArray  *    _pieceType;//工序
@property(nonatomic,strong) NSString *    _strIsShow;//1、显示加入企业 2、不显示加入企业
@property(nonatomic,strong) NSString *    _strIsAdmin;//1、普通员工 2、管理员

